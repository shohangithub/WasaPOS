@model IEnumerable<Model.ViewModels.WaterProductionViewModels>
<div class="col-xs-12">

    <div class="clearfix">
        <div class="pull-right tableTools-container"></div>
    </div>
    <div class="table-header btn-custom">
        Results for "Records List"
        <a class="pull-right btn-print" href="@Url.Action("WaterProductionRecords","Reports",new{id = "PDF", fd = @ViewBag.date, td = @ViewBag.tdate })" target="_blank">
            <i class="ace-icon fa fa-print bigger-130"></i> Print
        </a>

    </div>


    <table id="dynamic-table" class="table table-striped table-bordered table-hover">
        <thead>
            <tr>
                <th>Date</th>
                <th>Size </th>
                <th>Case And Quantity </th>

                <th>Quantity</th>
                <th>Amount (Litter)</th>
               


            </tr>
        </thead>

        <tbody>
            @if (Model.Count() > 0)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td>@Convert.ToDateTime(@item.ProductionDate).ToString("dd-MM-yyy")</td>
                        <td>@item.Size</td>
                        <td>@item.QtyCase</td>

                        <td>@item.Quantity</td>
                        <td>@item.Litter</td>
                       
                    </tr>
                }
            }
            else
            {
                <tr>
                    <td class="number" colspan="5"><h4>No Data Available!</h4></td>
                </tr>
            }


        </tbody>
    </table>

</div>

